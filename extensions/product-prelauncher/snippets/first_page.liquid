<div class="section-layout {{ block.settings.layout }}">
  <div class="hidden">{{ block.settings.campaign_name }}</div>
  <div id="snackbar">You have already signed up either from this email address or from the same IP</div>
  <div id="snackbar2">Email is not valid</div>
  <div id="snackbar3">Something went wrong</div>

  {% if block.settings.layout == 'none' %}
    <div id='background-image' class="none-layout none-parent-container">
      <div class="subheader_text">{{ block.settings.subheader_text }}</div>
      <div class="header_text">{{ block.settings.header_text }}</div>
      <div class="none">
        <div class="spaced">
          <div class="tag-line">{{ block.settings.cta_tag_text }}</div>
          <form
            action='/apps/xychrosupdated/api/getuser'
            id="form-submission"
            class="first-page-form">
            <input
              type="email"
              required
              id="email"
              name="email"
              placeholder="{{ block.settings.email_text }}"
              class='email-input-product-prelaucher' />
            {% if block.settings.phone_text != '' %}
              <input
                type="tel"
                required
                id="phone"
                name="phone"
                placeholder="{{ block.settings.phone_text }}"
                class='phone-input-product-prelaucher' />
            {% endif %}
            <div class="consent">
              <input
                type="checkbox"
                id="userconsent"
                name="userconsent"
                value="Agree"
                class='consent-input-product-prelaucher' />
              <label for="userconsent" class="consent-label-product-prelaucher">
                I consent to share my email address and phone number for marketing material</label>
            </div>
            {% if block.settings.show_social_icons == true %}
              <div class="social_icons">
                {% if block.settings.facebook_link != '' %}
                  <a class="social_icon" target="_blank" href={{ block.settings.facebook_link }}>
                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 30 30"><path d="M15 3a12 12 0 0 0-2 24v-9h-3v-3h3v-2c0-4 2-5 5-5h2v3h-2l-1 2v2h3v3h-3v9a12 12 0 0 0-2-24z"/></svg>
